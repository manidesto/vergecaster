{
  "manifest_version": 2,

  "name": "Vergecaster",
  "short_name": "Vergecaster",
  "description": "Does things that a guy does after drinking a bottle of Scissor Vodka",
  "version": "1.0",
  "icons": {
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
  },
  "content_scripts": [
      {
          "matches": ["https://www.theverge.com/*", "http://www.theverge.com/*"],
          "js": ["content.js"]
      }
  ]
}
